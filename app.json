{
  "expo": {
    "name": "SincroVzla",
    "slug": "silent-tyson",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sincrovzla",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a1e12"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sincroVzla.jooz",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "SincroVzla necesita tu ubicaci칩n para mostrarte precios cercanos a ti.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "SincroVzla necesita tu ubicaci칩n para mostrarte precios cercanos a ti.",
        "NSUserTrackingUsageDescription": "Este identificador se utilizar치 para ofrecerte anuncios personalizados."
      }
    },
    "android": {
      "package": "com.sincroVzla.jooz",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-dev-client",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.570425447392-l7k4mal4gqnf8g39mnv6u723i1rc1k9e"
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-4690411319254964~1168203165",
          "iosAppId": "ca-app-pub-4690411319254964~1168203165",
          "user_tracking_usage_description": "Este identificador se utilizar치 para ofrecerte anuncios personalizados."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c039b76d-3134-450f-b2b8-ac1020b916f2"
      }
    },
    "owner": "joropeza"
  }
}